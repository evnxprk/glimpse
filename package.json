    "@prisma/client": "^3.14.0",
    "@rpitv/glimpse-permissions": "^0.1.3",
    "body-parser": "^1.19.0",
    "class-validator": "^0.13.2",
    "connect-redis": "^6.1.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
